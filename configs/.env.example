### Application
APPLICATION_NAME=starter-kit
APPLICATION_URL=http://localhost:8000
ENV_NAME=development
VERSION=latest
IP=0.0.0.0
HTTP_PORT=8000
SHUTDOWN_TIMEOUT=3000
REQUEST_TIMEOUT=6000

### Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/starter_kit
DATABASE_TIMEOUT=5000
ENABLE_SEEDS=1

### Security
JWT_ACCESS_TOKEN_SECRET=your-access-token-secret-min-32-chars
JWT_ACCESS_TOKEN_EXPIRATION_TIME=15m
JWT_REFRESH_TOKEN_SECRET=your-refresh-token-secret-min-32-chars
JWT_REFRESH_TOKEN_EXPIRATION_TIME=30d
ENCRYPTION_KEY=your-encryption-key-minimum-32-characters

### Logging
LOG_LEVEL=debug
ENABLE_PRETTY_LOG=1
ENABLE_COLORIZED_LOG=1
ENABLE_DB_LOGGING=1
ENABLE_RESPONSE_LOGGING_BODY=1
ENABLE_DEVELOPER_MESSAGE=1
ENABLE_PERSISTENCE_FORCE_LOGGING=1
ENABLE_DEBUG=0

### Rate Limiting
RATE_LIMIT_MAX=10
RATE_LIMIT_TIME_WINDOW=60000

### Metrics (optional)
METRICS_API_KEY=

### CORS (optional)
ALLOWED_ORIGINS=
